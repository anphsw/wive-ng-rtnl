.EXPORT_ALL_VARIABLES:

CONSOLE_BAUD_RATE = 57600

MODEL_NAME		   = RT-NL
KERNEL_VER		   = "0.1.0"
FS_VER			   = "0.0"
PRODUCTID		   = "RT-NL"
HWL1			   = "0.1.0"
HWH1			   = "0.0"
HWL2			   = "0.0"
HWH2			   = "0.0"
HWL3			   = "0.0"
HWH3			   = "0.0"
HWL4			   = "0.0"
HWH4			   = "0.0"

MACHINE			   = mips
ARCH		           = mips
OPTS			   = -Os -pipe -mips32r2 -mtune=24kec
ROOTDIR			   ?= /opt/Wive-RTNL
include $(ROOTDIR)/vendors/config/mips/config.arch

FIRMROOT        	   = $(ROOTDIR)
LINUX_CONFIG		   = $(FIRMROOT)/linux/.config
TOOLCHAINDIR          	   = $(FIRMROOT)/toolchain
LINUXINCDIR                = $(FIRMROOT)/linux-2.6.21.x/include
CONFIG_CROSS_COMPILER_PATH = $(TOOLCHAINDIR)/bin

CFLAGS  += -I$(ROOTDIR)
CFLAGS  += -Wno-unused-but-set-variable
CFLAGS  += $(OPTS)
UOPT    += $(OPTS)
LOPT    += $(OPTS)
