.EXPORT_ALL_VARIABLES:

include $(ROOTDIR)/vendors/config/mips/config.arch

CONSOLE_BAUD_RATE = 57600

CFLAGS  += -I$(ROOTDIR)
CFLAGS  += -Os -pipe -mips32 -mtune=mips32
UOPT    += -Os -pipe -mips32 -mtune=mips32
LOPT    += -Os -pipe -mips32 -mtune=mips32

MODEL_NAME		   = RT-NL
KERNEL_VER		   = "0.1.0"
FS_VER			   = "0.0"
PRODUCTID		   = "RT-NL"
HWL1			   = "0.1.0"
HWH1			   = "0.0"
HWL2			   = "0.0"
HWH2			   = "0.0"
HWL3			   = "0.0"
HWH3			   = "0.0"
HWL4			   = "0.0"
HWH4			   = "0.0"

CONFIG_NOUSB		   = y

MACHINE			   = mips
ARCH		           = mips
ROOTDIR			   = /opt/Wive-RTNL
FIRMROOT        	   = $(ROOTDIR)
TOOLCHAINDIR          	   = $(FIRMROOT)/toolchain
LINUXINCDIR                = $(FIRMROOT)/linux-2.6.21.x/include
CONFIG_CROSS_COMPILER_PATH = $(TOOLCHAINDIR)/bin
#STRIP			   = $(FIRMROOT)/tools/sstrip/sstrip
