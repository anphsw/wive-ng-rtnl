#!/bin/sh

PWD=`pwd`

rm -rf $PWD/releases
mkdir -p $PWD/releases

#DEVLIST="RT3662-2T2R-5G-USB RT3052-2T2R RT3052-2T2R-USB RT3050-1T1R"
DEVLIST="RT3662-2T2R-5G-USB RT3052-2T2R RT3052-2T2R-USB RT3050-1T1R RT3352-2T2R-USB RT5350-1T1R"

./clear

for devname in $DEVLIST
do
    ./clear RELEASE
    nice -19 ./compile "$devname"
    cp -f $PWD/images/*.zip $PWD/releases
done
