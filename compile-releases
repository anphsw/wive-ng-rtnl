#!/bin/sh

PWD=`pwd`

rm -rf $PWD/releases
mkdir $PWD/releases

./clear
nice -19 ./compile W532G
cp -f $PWD/images/*.zip $PWD/releases

./clear
nice -19 ./compile W530G
cp -f $PWD/images/*.zip $PWD/releases

./clear
nice -19 ./compile W422G_v3
cp -f $PWD/images/*.zip $PWD/releases

./clear
nice -19 ./compile W422G_v4
cp -f $PWD/images/*.zip $PWD/releases

./clear
nice -19 ./compile LAN122_v3
cp -f $PWD/images/*.zip $PWD/releases
