#!/bin/sh

PWD=`pwd`

rm -rf $PWD/releases
mkdir -p $PWD/releases

./clear
nice -19 ./compile RT3050-1T1R
cp -f $PWD/images/*.zip $PWD/releases

./clear RELEASE
nice -19 ./compile RT3052-2T2R
cp -f $PWD/images/*.zip $PWD/releases

./clear RELEASE
nice -19 ./compile RT3052-2T2R-USB
cp -f $PWD/images/*.zip $PWD/releases

./clear RELEASE
nice -19 ./compile RT3662-2T2R-5G-USB
cp -f $PWD/images/*.zip $PWD/releases
