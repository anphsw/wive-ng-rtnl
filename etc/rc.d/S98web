#!/bin/sh

start() {
 echo "Start goahead"
    killall nvram_daemon > /dev/null 2>&1
    /bin/goahead &

 echo "Unload unneeded modules"
    rmmod -a
}

stop() {
 echo "Stop goahead"
    killall -9 goahead > /dev/null 2>&1
    killall -9 nvram_daemon > /dev/null 2>&1
}

case "$1" in
	start)
	    start
	    ;;

	stop)
	    stop
	    ;;

	restart)
	    stop
	    start
	    ;;

	*)
	    echo $"Usage: $0 {start|stop|restart}"
	    exit 1
esac
